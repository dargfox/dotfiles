{
  "codeLens.enable": true,
  "diagnostic.errorSign": "ⓧ",
  "diagnostic.infoSign": "ⓘ",
  "diagnostic.hintSign": "ⓗ",
  "diagnostic.enableHighlightLineNumber": true,
  "signature.enable": true,
  "coc.preferences.colorSupport": true,
  "highlight.colors.enable": true,
  "hover.floatConfig": {
    "border": true
  },
  "diagnostic.floatConfig": {
    "border": true
  },
  "suggest.floatConfig": {
    "border": true
  },
  "signature.floatConfig": {
    "border": true
  },
  "pyright.enable": true,
  "pyright.inlayHints.parameterTypes": true,
  "pyright.disableDiagnostics": false,
  "python.linting.mypyEnabled": false,
  "python.linting.ruffEnabled": true,
  "python.linting.pytypeEnabled": true,
  "python.linting.pylintEnabled": true,
  "pyright.organizeimports.provider": "ruff",
  "python.formatting.provider": "ruff",
  "css.lint.unknownAtRules": "ignore",
  "scss.lint.unknownAtRules": "ignore",
  "tailwindCSS.cssLanguages": [ "css", "less", "postcss", "sass", "scss", "stylus", "vue" ],
  "tailwindCSS.jsLanguages": [ "javascript", "javascriptreact", "reason", "typescriptreact" ],
  "tailwindCSS.htmlLanguages": [ "blade", "edge", "eelixir", "ejs", "elixir", "elm", "erb", "eruby", "haml", "handlebars", "htmldjango", "html", "HTML (EEx)", "HTML (Eex)", "html.twig", "jade", "leaf", "markdown", "njk", "nunjucks", "php", "razor", "slim", "svelte", "twig", "vue" ],
}
